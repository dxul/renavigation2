{
  "name": "@renavigation2/history",
  "version": "4.3.4",
  "description": "Manage session history with JavaScript",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "keywords": [],
  "repository": "https://github.com/gabrielbull/renavigation2",
  "author": "React Training <hello@reacttraining.com>",
  "contributors": [
    "Gabriel Bull <me@gabrielbull.com> (https://github.com/gabrielbull)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gabrielbull/renavigation2/issues"
  },
  "homepage": "https://github.com/gabrielbull/renavigation2#readme",
  "files": [
    "src",
    "lib",
    "!**/__tests__"
  ],
  "scripts": {
    "build": "bob build",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "release": "release-it",
    "test": "jest",
    "typescript": "tsc --noEmit",
    "watch": "nodemon -e tx,tsx,js,jsx --watch src --exec yarn build"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^8.3.4",
    "@react-native-community/bob": "^0.16.2",
    "@release-it/conventional-changelog": "^1.1.4",
    "@types/jest": "^26.0.0",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "babel-preset-jest": "^26.1.0",
    "commitlint": "^8.3.5",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.6",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "metro-react-native-babel-preset": "^0.60.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "release-it": "^13.5.8",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn lint && yarn typescript"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "@react-native-community/bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  }
}
